{
    "name": "vscode-jest-runner",
    "displayName": "vscode-jest-runner",
    "description": "Run and check result of your Jest tests",
    "version": "0.0.1",
    "publisher": "Gilthoniel",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:javascript",
        "onLanguage:javascriptreact"
    ],
    "main": "./src/extension",
    "contributes": {
        "commands": [
            {
                "command": "jest-runner.run-test",
                "title": "JestRunner: Run Tests"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "babylon": "^6.18.0",
        "child_process": "^1.0.2",
        "has": "^1.0.1"
    }
}
